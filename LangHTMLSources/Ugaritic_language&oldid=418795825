				<!-- /jumpto -->
								<!-- bodytext -->
				<div class="dablink">Not to be confused with the unrelated <a href="/wiki/Ugric_languages" title="Ugric languages">Ugric languages</a>.</div>
<table class="infobox" style="width: 22em; text-align: left; font-size: 88%; line-height: 1.5em">
