						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading">Etruscan language</h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
				<!-- tagline -->
				<div id="siteSub">From Wikipedia, the free encyclopedia</div>
				<!-- /tagline -->
				<!-- subtitle -->
				<div id="contentSub"></div>
				<!-- /subtitle -->
																<!-- jumpto -->
				<div id="jump-to-nav">
					Jump to: <a href="#mw-head">navigation</a>,
					<a href="#p-search">search</a>
				</div>
				<!-- /jumpto -->
								<!-- bodytext -->
				<table class="infobox" style="width: 22em; text-align: left; font-size: 88%; line-height: 1.5em">
<tr>
<th colspan="3" style="text-align: center; font-size: 125%; font-weight: bold; color: black; background-color: #dddddd">Etruscan</th>
</tr>
<tr>
<th colspan="3" style="text-align: center; font-weight: bold; color: black; background-color: #dddddd">mechl Rasnal</th>
</tr>
<tr>
<th>Spoken in</th>
