					<a href="#p-search">search</a>
				</div>
				<!-- /jumpto -->
								<!-- bodytext -->
				<table class="infobox" style="width: 22em; text-align: left; font-size: 88%; line-height: 1.5em">
<tr>
<th colspan="3" style="text-align: center; font-size: 125%; font-weight: bold; color: black; background-color: lightblue">Chamacoco</th>
</tr>
<tr>
<th>Spoken in</th>
