<td rowspan="2"><i>kteb-tiw</i></td>
<td><i>te-kteb</i></td>
<td rowspan="2"><i><b>t-ketb</b>-u</i></td>
<td><i>ka-te-kteb</i></td>
<td rowspan="2"><i>ka-t-ketb-u</i></td>
<td><i>ɣa-te-kteb</i></td>
<td rowspan="2"><i>ɣa-t-ketb-u</i></td>
