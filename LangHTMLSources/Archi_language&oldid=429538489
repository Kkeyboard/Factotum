						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading">Archi language</h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
				<!-- tagline -->
				<div id="siteSub">From Wikipedia, the free encyclopedia</div>
