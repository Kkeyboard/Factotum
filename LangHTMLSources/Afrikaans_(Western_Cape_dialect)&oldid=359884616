						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading">Afrikaans (Western Cape dialect)</h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
				<!-- tagline -->
